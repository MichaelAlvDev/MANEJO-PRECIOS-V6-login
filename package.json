{
  "name": "electron-precios-app",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "start:dev": "concurrently \"npm run start:backend:dev\" \"npm run start:electron:dev\"",
    "start:backend": "node backend/server.js",
    "start:backend:dev": "nodemon backend/server.js",
    "start:electron": "electron .",
    "start:electron:dev": "cross-env NODE_ENV=development electron .",
    "check-server": "node check-server.js",
    "kill-server": "node kill-server.js",
    "test-build": "node test-build.js",
    "test-secondary": "node test-secondary-windows.js",
    "test-db-config": "node test-db-config.js",
    "build": "electron-builder",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "bootstrap": "^5.3.6",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "mssql": "^11.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^30.0.0",
    "electron-builder": "^26.0.12",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2"
  },
  "build": {
    "appId": "com.tuempresa.electronpreciosapp",
    "productName": "Contol Precios App",
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "files": [
      "**/*",
      "!.db-config"
    ],
    "extraFiles": [],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    }
  }
}
